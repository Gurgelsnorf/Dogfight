#lang racket
(provide airplane%)


(define airplane%
  (class object%
    (init-field
     speed
     direction
     bl_corner
     br_corner
     tl_corner
     tr_corner)

    (define/public ($Get_Speed)
      speed)

    (define/public ($Get_Direction)
      direction)

    (define/public ($Get_Bl_Corner)
      bl_corner)

    (define/public ($Get_Br_Corner)
      br_corner)

    (define/public ($Get_Tl_Corner)
      tl_corner)

    (define/public ($Get_Tr_Corner)
      tr_corner)
    (super new)))
     